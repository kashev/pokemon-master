#:kivy 1.0.9

FloatLayout:
    canvas:
        Color:
            rgb: 1, 1, 1
        Rectangle:
            source: 'data/images/whos_that_pokemon_blank.jpg'
            size: self.size

<-FullImage>:
    canvas:
        Color:
            rgb: (1, 1, 1)
        Rectangle:
            texture: self.texture
            pos: self.x - 10, self.y - 10
